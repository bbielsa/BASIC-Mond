var lexer = require("lexer.mnd");
var parser = require("parser.mnd");
var l = lexer.new(readLn());
var tokens = l.scan();
var p = parser.new(tokens);
var ast = [];

for(var line = 0; line < l.input.split("\n").length(); line++) {
    ast.add(p.parseLineExpression());
}

foreach(var e in ast)
    e[1].run();
